{"components/blog/post":"<article class=\"blog\">\n    <div class=\"blog-post-figure\">\n        {{#if post.thumbnail}}\n            <figure class=\"blog-thumbnail\">\n                <!--<a href=\"{{post.url}}\">-->\n                    <img class=\"lazyload\" data-sizes=\"auto\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"{{getImage post.thumbnail 'blog_size'}}\" alt=\"{{post.thumbnail.alt}}\" title=\"{{post.thumbnail.alt}}\">\n               <!-- </a>-->\n            </figure>\n        {{/if}}\n    </div>\n\n    <div class=\"blog-post-body\">\n        <header class=\"blog-header\">\n            <h1 class=\"blog-title\">\n                <a href=\"{{post.url}}\">{{post.title}}</a>\n            </h1>\n           <!-- <p class=\"blog-date\">{{#if post.author}}{{lang 'blog.posted_by' name=post.author}} on {{/if}}{{post.date_published}}</p> -->\n        </header>\n\n        <div class=\"blog-post\">\n            {{#if post.body}}\n                {{{post.body}}}\n            {{else}}\n                {{{post.summary}}}\n                {{#if post.show_read_more}}\n                    &hellip; <a href=\"{{url}}\">read more</a>\n                {{/if}}\n            {{/if}}\n        </div>\n\n\n        {{#if show_tags}}\n            {{#if post.tags}}\n                <ul class=\"tags\">\n                    {{#each post.tags}}\n                    <li class=\"tag\">\n                        <a href=\"{{url}}\">#{{name}}</a>\n                    </li>\n                    {{/each}}\n                </ul>\n            {{/if}}\n        {{/if}}\n\n        {{>components/common/share}}\n\n    </div>\n</article>\n","components/common/share":"{{#if settings.add_this.buttons}}\n    <div>\n        <ul class=\"socialLinks\">\n            {{#each settings.add_this.buttons}}\n                {{#if service '===' 'google_plusone'}}\n                    <li class=\"socialLinks-item socialLinks-item--{{service}}\">\n                        <a target=\"_blank\" href=\"https://plus.google.com/share?url={{../../url}}\"\n                        class=\"google-plus icon\">\n                            <svg class=\"icon-social\">\n                                <use xlink:href=\"#icon-google-plus\"></use>\n                            </svg>\n                        </a>\n                    </li>\n                {{/if}}\n                \n                {{#if service '===' 'facebook'}}\n                    <li class=\"socialLinks-item socialLinks-item--{{service}}\">\n                        <a target=\"_blank\" href=\"http://www.facebook.com/sharer.php?u={{../../url}}\"\n                        class=\"facebook icon\">\n                            <svg class=\"icon-social\">\n                                <use xlink:href=\"#icon-facebook\"></use>\n                            </svg>\n                        </a>\n                    </li>\n                {{/if}}\n                \n                {{#if service '===' 'email'}}\n                    <li class=\"socialLinks-item socialLinks-item--{{service}}\">\n                        <a href=\"mailto:?subject={{../../title}}&amp;body={{../../url}}\"\n                        class=\"email icon\">\n                            <svg class=\"icon-social\">\n                                <use xlink:href=\"#icon-envelope\"></use>\n                            </svg>\n                        </a>\n                    </li>\n                {{/if}}\n                \n                {{#if service '===' 'print'}}\n                    <li class=\"socialLinks-item socialLinks-item--{{service}}\">\n                        <a class=\"print icon\" href=\"javascript:;\" onclick=\"window.print()\">\n                            <svg class=\"icon-social\">\n                                <use xlink:href=\"#icon-print\"></use>\n                        </svg>\n                        </a>\n                    </li>\n                {{/if}}\n                \n                {{#if service '===' 'twitter'}}\n                    <li class=\"socialLinks-item socialLinks-item--{{service}}\">\n                        <a target=\"_blank\" href=\"https://twitter.com/share?url={{../../url}}&amp;text={{../../title}}\" class=\"twitter icon\">\n                            <svg class=\"icon-social\">\n                                <use xlink:href=\"#icon-twitter\"></use>\n                            </svg>\n                        </a>\n                    </li>\n                {{/if}}\n                \n                {{#if service '===' 'linkedin'}}\n                    <li class=\"socialLinks-item socialLinks-item--{{service}}\">\n                        <a target=\"_blank\" href=\"http://www.linkedin.com/shareArticle?url={{../../url}}&amp;title={{../../title}}\"\n                        class=\"linkedin icon\">\n                            <svg class=\"icon-social\">\n                                <use xlink:href=\"#icon-linkedin\"></use>\n                            </svg>\n                        </a>\n                    </li>\n                {{/if}}\n                \n                {{#if service '===' 'pinterest'}}\n                    <li class=\"socialLinks-item socialLinks-item--{{service}}\">\n                        <a target=\"_blank\" href=\"//pinterest.com/pin/create/button/?url={{../../url}}&amp;media={{../../image}}&amp;description={{../../title}}\" class=\"share-pinterest icon\">\n                            <svg class=\"icon-social\">\n                                <use xlink:href=\"#icon-pinterest\"></use>\n                            </svg>\n                        </a>\n                    </li>\n                {{/if}}\n                \n                {{#if service '===' 'facebook_like'}}\n                    <li class=\"socialLinks-item socialLinks-item--{{service}}\">\n                        <div class=\"fb-like\"\n                            data-href=\"{{../../url}}\"\n                            data-layout=\"button_count\"\n                            data-action=\"like\"\n                            data-show-faces=\"false\">\n                        </div>\n                    </li>\n                {{/if}}\n            {{/each}}\n        </ul>\n    </div>\n{{/if}}\n"}
